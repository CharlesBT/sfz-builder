  },
  "license": "MIT",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf ./dist *.tsbuildinfo && tsc",
    "dev": "rimraf ./dist && tsc --watch",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ci": "vitest run",
    "lint": "eslint --fix .",
    "format": "prettier --write .",
    "doc:serve": "serve -l 3030 ./docs",
    "doc:build": "typedoc",
    "doc:watch": "typedoc --watch",
    "clean": "rimraf ./dist ./build ./.cache ./.temp ./logs ./test/coverage",
    "postinstall": "pnpm build"
  },
  "dependencies": {
    "adm-zip": "^0.5.9",
    "debug": "^4.3.4",
    "ffmpeg-static-electron": "^2.0.3",
    "ffprobe-static-electron": "^2.0.2",
    "filenamify": "^5.1.1",
    "fs-extra": "^10.1.0",
    "lodash": "^4.17.21",
    "pkg-dir": "7.0.0",
    "uuid": "^9.0.0",
    "wavefile": "^11.0.0"
  },
  "devDependencies": {
    "@charlesbt/eslint-config-custom": "latest",
    "@charlesbt/prettier-config-custom": "latest",
    "@microsoft/tsdoc": "^0.14.2",
    "@types/adm-zip": "^0.5.0",
    "@types/debug": "^4.1.7",
