{
  "name": "@bms/audio",
  "version": "0.1.0",
  "description": "BMS Audio",
  "repository": {
    "type": "git",
    "url": "https://github.com/CharlesBT/bms.git"
  },
  "keywords": [],
  "author": {
    "name": "Charles Br√©bant",
