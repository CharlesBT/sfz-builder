    "@types/uuid": "^8.3.4",
    "@vitest/coverage-c8": "^0.24.5",
    "serve": "^14.1.2",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.21",
    "typescript": "^4.9.3",
    "vitest": "^0.24.5"
  }
}
